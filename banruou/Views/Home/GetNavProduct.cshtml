@model banruou.ViewModel.GetNavProduct

<div class="list-category post-name-related">
    <div class="list-category-title">
        <h3>
            Danh mục sản phẩm
        </h3>
    </div>
    <ul class="list-item-category">
        @foreach (var item in Model.ProductCategories.Take(5))
        {
            <li><a href="@Url.Action( "CategoryProduct" ,"Home" , new { url = item.Url })">@item.CategoryName</a></li>
            <hr>
        }
    </ul>
</div>

